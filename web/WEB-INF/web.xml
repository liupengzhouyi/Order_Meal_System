         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>getUserRegisteredInformationServlet</servlet-name>
        <servlet-class>Servlet.getUserRegisteredInformationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getUserRegisteredInformationServlet</servlet-name>
        <url-pattern>/Servlet/getUserRegisteredInformationServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getLandingInformationServle</servlet-name>
